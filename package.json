{
  "name": "point-tracker",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "cleanApiKeys": "node ./scripts/postbuild/postbuild.js",
    "buildPreviewAndroid": "node ./scripts/prebuild/prebuild.js && eas build -p android --profile preview && node ./scripts/postbuild/postbuild.js",
