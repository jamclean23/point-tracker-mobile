  },
  "dependencies": {
    "@react-navigation/native": "^6.1.17",
    "chalk": "^5.3.0",
    "expo": "^51.0.9",
    "expo-build-properties": "~0.12.5",
    "expo-secure-store": "~13.0.1",
    "expo-status-bar": "~1.12.1",
    "nanoid": "^5.0.7",
    "react": "18.2.0",
    "react-native": "0.74.1",
    "react-native-maps": "1.14.0",
    "react-native-uuid": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "react-native-dotenv": "^3.4.11"
  },
  "private": true
}
